				<value>두번째</value>
				<value>세번째</value>
				<value>네번째</value>
				<ref bean="item1"/>
				<ref bean="item2"/>
				<ref bean="item3"/>
				<bean id="item4" class="java.util.Date"></bean>
			</set>
		</property>
		
		<property name="arr3">
			<map>
				<entry key="key1" value="val11"></entry>
				<entry>
					<key>
						<value>key2</value>
					</key>
					<value>val2</value>
				</entry>
				<entry key="key3">
					<bean class="java.util.Date"></bean>
				</entry>
				<entry key="key4" value-ref="item3"></entry>
				<entry>
					<key>
						<value>key5</value>
					</key>
					<ref local="item3"/>
				</entry>
				
			</map>
		</property>
		
		<property name="arr4">
			<!-- <map>
