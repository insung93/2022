	<bean id="item1" class="java.util.Date"></bean>
	<bean id="item2" class="java.util.Date"></bean>
	<bean id="item3" class="java.util.Date"></bean>

	<bean id="bean" class="com.bit.emp.model.EmpVo">
		<property name="arr1">
			<list>
				<value>1111</value>
				<value>2222</value>
				<value>3333</value>
				<value>4444</value>
				<value>첫번째</value>
				<value>두번째</value>
				<value>세번째</value>
				<value>네번째</value>
				<ref bean="item1"/>
				<ref bean="item2"/>
				<ref bean="item3"/>
				<bean id="item4" class="java.util.Date"></bean>
			</list>
		</property>
		
		<property name="arr2">
			<set>
				<value>1111</value>
				<value>2222</value>
				<value>3333</value>
				<value>4444</value>
